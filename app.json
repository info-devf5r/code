{
  "name": "24/7 live stream radio",
  "description": "A sample live stream radio app",
  "repository": "https://github.com/tonmoyislam12/youtube-live-24",
  "stack": "container",
  "env": {
    "STREAM_KEY": {
      "description": "KEY FOR STREAMING",
      "required": true
    },
    "STREAM_URL": {
      "description": "STREAM URL without leading '/'",
      "required": true
    },
    "LINK": {
      "description": "gdrive link",
      "required": true
    },
    "FOLDER_NAME": {
      "description": "folder name of the link",
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "eco"
    }
  }
}
